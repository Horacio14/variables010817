
public class variables010817 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		byte edad=25; //declaracion e iniciacion de variable en una linea
		
		/* con esto puedo escribir varias lineas de comentarios, por ejemplo que la
		  siguiente instruccion imprimie en la consola (System.out) el valor de la
		  variable edad
		 */
		
		System.out.println(edad);
		
		
	}

}
